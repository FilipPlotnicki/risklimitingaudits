{
  "webroot": "wwwroot/dist",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta7"
  },

  "commands": {
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --config hosting.ini"
  },

  "frameworks": {
    "dnxcore50": {
      "dependencies": {
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta7"
      }
    }
  },

  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
